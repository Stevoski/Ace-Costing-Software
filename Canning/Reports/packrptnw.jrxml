	<queryString language="SQL">
		<![CDATA[SELECT 
        `packed_product`.`packed_productid` AS `packed_productid`,
        `packed_product`.`date` AS `date`,
        `packed_product`.`factory` AS `factory`,
        `packed_product`.`packed_product` AS `packed_product`,
        `packed_product`.`packed_qty` AS `packed_qty`,
        `packed_product`.`manpower_hrs` AS `manpower_hrs`,
        `packing_consumed`.`consumed_id` AS `consumed_id`,
        `packing_consumed`.`item_name` AS `item_name`,
        `packing_consumed`.`item_qty` AS `item_qty`,
        `packing_package`.`package` AS `package`,
        `packing_package`.`package_qty` AS `package_qty`,
        `packing_package`.`label` AS `label`,
        `packing_package`.`label_qty` AS `label_qty`
    FROM
        ((`packed_product`
        LEFT JOIN `packing_consumed` ON ((`packing_consumed`.`pprodid` = `packed_product`.`packed_productid`)))
        LEFT JOIN `packing_package` ON (((`packed_product`.`packed_productid` = `packing_package`.`ref_packid`)
            AND (`packing_package`.`ref_packid` = `packing_consumed`.`pprodid`))))
            where `packed_product`.`packed_productid` = '5'
    GROUP BY `packed_product`.`packed_productid`]]>
	</queryString>
