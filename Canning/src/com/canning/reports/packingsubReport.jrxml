		<parameter name="spar1" class="java.lang.Long"/>
		<queryString>
			<![CDATA[SELECT 
        `packed_product`.`packed_productid` AS `PackID`,
        `packed_product`.`date` AS `date`,
        `packed_product`.`factory` AS `factory`,
        `packed_product`.`packed_product` AS `packedproduct`,
        `packed_product`.`packed_qty` AS `packedqty`,
        `packed_product`.`manpower_hrs` AS `manpower_hrs`,
        `packing_consumed`.`item_name` AS `consumedproduct`,
        `new_view`.`Cperunit` AS `consumedprice`,
        (`packing_consumed`.`item_qty` * `new_view`.`Cperunit`) AS `consumedamount`,
        `packing_consumed`.`item_qty` AS `consumedqty`,
        `packing_consumed`.`consumed_id` AS `ProductionID`, 
        `packing_package`.`package` AS `package`,
        `packing_package`.`label` AS `label`,
        `packing_package`.`package_qty` AS `packageqty`,
        `packing_package`.`label_qty` AS `labelqty`,
        (`product`.`kgsperpc` * `packing_consumed`.`item_qty`) AS `quantityKGS`
    FROM
        ((((`packed_product`
        LEFT JOIN `product` ON ((`packed_product`.`packed_product` = CONVERT( `product`.`product_name` USING UTF8MB4))))
        LEFT JOIN `packing_consumed` ON ((`packed_product`.`packed_productid` = `packing_consumed`.`pprodid`)))
        LEFT JOIN `new_view` ON ((`new_view`.`Production_ID` = `packing_consumed`.`consumed_id`)))
        LEFT JOIN `packing_package` ON ((`packed_product`.`packed_productid` = `packing_package`.`ref_packid`)))
    GROUP BY `packed_product`.`packed_productid`]]>
