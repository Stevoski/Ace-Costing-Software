	<queryString>
		<![CDATA[SELECT 
        `packingreport`.`ProductionID` AS `ProductionID`,
        `packingreport`.`packrptid` AS `PackID`,
        `packingreport`.`date` AS `date`,
        `packingreport`.`packedproduct` AS `packedproduct`,
        `packingreport`.`packedqty` AS `packedqty`,
        `packingreport`.`consumedproduct` AS `consumedproduct`,
        `new_view`.`Cperunit` AS `consumedprice`,
        (`packingreport`.`consumedqty` * `new_view`.`Cperunit`) AS `consumedamount`,
        `packingreport`.`consumedqty` AS `consumedqty`,
   `packingreport`.`package` AS `package`,
        `packingreport`.`label` AS `label`,
       `packingreport`.`packageqty` AS `packamount`,
        (`packingreport`.`labelprice` * `packingreport`.`labelqty`) AS `labamount`,
        (`product`.`kgsperpc` * `packingreport`.`packedqty`) AS `quantityKGS`,
        TRUNCATE((`packingreport`.`manpower` * '35'),
            2) AS `manamount`,
         `packingreport`.`labelqty` AS `labelqty`,
        TRUNCATE(((`packingreport`.`consumedqty` * `new_view`.`Cperunit`) / `packingreport`.`packedqty`),
            2) AS `procost`,
        TRUNCATE(((`packingreport`.`packageprice` * `packingreport`.`packageqty`) / `packingreport`.`packedqty`),
            2) AS `packagecost`,
        TRUNCATE(((`packingreport`.`manpower` * '35') / `packingreport`.`packedqty`),
            2) AS `mancost`,
        TRUNCATE((((((`packingreport`.`labelprice` * `packingreport`.`labelqty`) / `packingreport`.`packedqty`) + ((`packingreport`.`consumedqty` * `new_view`.`Cperunit`) / `packingreport`.`packedqty`)) + ((`packingreport`.`packageprice` * `packingreport`.`packageqty`) / `packingreport`.`packedqty`)) + ((`packingreport`.`manpower` * '35') / `packingreport`.`packedqty`)),
            2) AS `PackingCost`
    FROM
        ((`packingreport`
        LEFT JOIN `product` ON ((`packingreport`.`packedproduct` = `product`.`product_name`)))
        LEFT JOIN `new_view` ON ((`new_view`.`Production_ID` = `packingreport`.`ProductionID`)))
where `new_view`.`voucher_no`='405'
    ORDER BY `packingreport`.`ProductionID`]]>
	</queryString>
